{
  "version": "0.0.1",
  "name": "pseudo-immutable-array",
  "id": "pseudo-immutable-array",
  "description": "Immutable alternatives to built-in array operators",
  "keywords": [ "pseudo-immutable-array" ],
  "author": "Irakli Gozalishvili <rfobic@gmail.com> (http://jeditoolkit.com)",
  "homepage": "https://github.com/Gozala/pseudo-immutable-array",
  "main": "./lib/pseudo-immutable-array.js",
  "devDependencies": {
    "babel-cli": "6.11.4",
    "babel-plugin-transform-class-properties": "6.11.5",
    "babel-preset-es2015": "6.3.13",
    "babel-preset-flow": "1.0.0",
    "babel-register": "6.11.6",
    "flow-bin": "0.30.0",
    "flow-copy-source": "1.1.0",
    "standard-flow": "1.0.0",
    "tape": "4.4.0"
  },
  "scripts": {
    "test": "npm run flow && npm run lint && npm run tape",
    "tape": "tape -r babel-register test/**/*.js",
    "flow": "flow check",
    "lint": "standard-flow src/*.js",
    "start": "babel --watch --out-dir lib src",
    "copy-types": "flow-copy-source src lib",
    "build-node": "babel --out-dir lib src",
    "build": "npm run build-node && npm run copy-types",
    "prepublish": "npm run build && npm test"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Gozala/pseudo-immutable-array.git",
    "web": "https://github.com/Gozala/pseudo-immutable-array"
  },
  "bugs": {
    "url": "http://github.com/Gozala/pseudo-immutable-array/issues/"
